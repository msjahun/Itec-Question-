) #include <stdio.h>
#include <stdlib.h>

void main(){
 printf("Please Input an Integer Number: \n");
  int num;
  for(;1;){
  scanf("%d",&num);

   if(num>0){

   printf("%d:",num);
    for(int i=0;i<num;i+=5){
        printf("X");}

   }
   else{
    printf("%d:End of Program",num);
	break;}
  }
system("pause");
}

